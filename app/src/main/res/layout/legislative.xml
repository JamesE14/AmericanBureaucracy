<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/legislative"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_red_light">

    <ImageView
        android:id="@+id/legLogo"
        android:layout_width="98dp"
        android:layout_height="81dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/congressicon" />

    <TableLayout
        android:id="@+id/mainTable"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TableRow
            android:id="@+id/table1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:id="@+id/table10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </TableLayout>


</android.support.constraint.ConstraintLayout>