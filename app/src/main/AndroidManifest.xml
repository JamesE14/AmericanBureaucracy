    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/congresssssiconnew_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/congresssssiconnew_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name="pro.jamesegger.americanbureaucracy.executiveRelated"
            android:parentActivityName="pro.jamesegger.americanbureaucracy.MainActivity">
        </activity>


        <activity
            android:name="pro.jamesegger.americanbureaucracy.judicialRelated"
            android:parentActivityName="pro.jamesegger.americanbureaucracy.MainActivity" >
        </activity>

        <activity
            android:name="pro.jamesegger.americanbureaucracy.legislativeRelated"
            android:parentActivityName="pro.jamesegger.americanbureaucracy.MainActivity" >
        </activity>

    </application>

</manifest>